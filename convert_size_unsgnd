#include "main.h"
/**
 * convert_size_unsgnd - sends number to specified size
 * @num: Number sent
 * @size: Number type being sent
 *
 * Return: sent value of num
 */
long int convert_size_unsgnd(unsigned long int num, int size)
{
	if (size == S_LONG)
		return (num);
	else if (size == S_SHORT)
		return ((unsigned short)num);

	return ((unsigned int)num);
}
